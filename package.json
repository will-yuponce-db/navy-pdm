{
  "name": "navy-pdm",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router build && react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:smoke": "playwright test",
    "test:e2e": "playwright test",
    "test:selenium": "tsx tests/selenium/test-runner.ts",
    "test:selenium:home": "tsx tests/selenium/home-page.test.ts",
    "test:selenium:workorder": "tsx tests/selenium/work-order.test.ts",
    "test:selenium:parts": "tsx tests/selenium/parts.test.ts",
    "lint": "eslint app --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint app --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"app/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"app/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "security:scan": "npm audit && snyk test",
    "performance:lighthouse": "lhci autorun",
    "performance:bundle": "webpack-bundle-analyzer build/client/assets/*.js",
    "pwa:generate-icons": "pwa-asset-generator public/icon.png public/icons",
    "pwa:validate": "pwa-validator public/manifest.json",
    "docker:build": "docker build -t navy-pdm .",
    "docker:run": "docker run -p 3000:3000 navy-pdm",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "deploy:staging": "npm run build && npm run docker:build && docker push ghcr.io/navy-pdm:staging",
    "deploy:production": "npm run build && npm run docker:build && docker push ghcr.io/navy-pdm:latest",
    "clean": "rm -rf build dist node_modules/.cache",
    "clean:all": "rm -rf build dist node_modules package-lock.json && npm install",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "db:migrate": "npm run typecheck && node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "backup:create": "node scripts/backup.js",
    "backup:restore": "node scripts/restore.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.2",
    "@mui/x-data-grid": "^8.11.0",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@reduxjs/toolkit": "^2.9.0",
    "@types/leaflet": "^1.9.20",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "isbot": "^5.1.27",
    "leaflet": "^1.9.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.7.1",
    "recharts": "^3.2.1",
    "uuid": "^11.1.0",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/selenium-webdriver": "^4.1.28",
    "@vitest/coverage-v8": "^3.2.4",
    "chromedriver": "^140.0.4",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "geckodriver": "^6.0.1",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "jsdom": "^27.0.0",
    "prettier": "3.6.2",
    "selenium-webdriver": "^4.35.0",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.42.0",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
