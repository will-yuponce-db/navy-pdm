{
  "name": "navy-pdm",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "dev:api": "node server.js",
    "dev:stack": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "react-router build",
    "build:prod": "VITE_API_URL=/api react-router build",
    "start": "npm run build:prod && NODE_ENV=production PORT=8000 node server.js",
    "start:split": "npm run build:prod && concurrently --names \"FRONTEND,API\" --prefix-colors \"cyan,green\" \"npm run start:frontend\" \"npm run start:api\"",
    "start:frontend": "PORT=8000 node server-frontend.js",
    "start:api": "PORT=3000 node server.js",
    "start:prod": "node scripts/start-production.js",
    "start:dev": "node server.js",
    "seed": "node api/seed.js",
    "test": "vitest",
    "lint": "eslint app --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"app/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@databricks/sql": "^1.12.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.2",
    "@mui/x-data-grid": "^8.11.0",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@reduxjs/toolkit": "^2.9.0",
    "@types/leaflet": "^1.9.20",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "better-sqlite3": "^11.6.0",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "isbot": "^5.1.27",
    "leaflet": "^1.9.4",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.7.1",
    "recharts": "^3.2.1",
    "uuid": "^11.1.0",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.1.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "jsdom": "^27.0.0",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.4",
    "terser": "^5.44.0",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.42.0",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
